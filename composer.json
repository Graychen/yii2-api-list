{
    "name": "graychen/yii-api-list",
    "description": "show the api list",
    "type": "yii-extension",
    "require-dev": {
        "phpunit/phpunit": "^7.1@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Graychen",
            "email": "13780185250@sina.cn"
        }
    ],
    "minimum-stability": "dev",
    "require": {
          "yiisoft/yii2": ">=2.0.6"
    },
    "autoload":{
      "psr-4": {
            "graychen\\yii2\\apilist\\":"src"
       }
